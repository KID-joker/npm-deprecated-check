{
  "name": "npm-deprecated-check",
  "type": "module",
  "version": "1.1.0",
  "description": "Check for deprecated packages",
  "author": "KID-joker <kid_joker@126.com>",
  "license": "MIT",
  "homepage": "https://github.com/KID-joker/npm-deprecated-check#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KID-joker/npm-deprecated-check.git"
  },
  "bugs": "https://github.com/KID-joker/npm-deprecated-check/issues",
  "keywords": [
    "cli",
    "cli-tool",
    "dependencies",
    "deprecated",
    "OpenAI",
    "ai-recommend"
  ],
  "main": "dist/cli.mjs",
  "module": "dist/cli.mjs",
  "bin": {
    "ndc": "bin/ndc.mjs"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10.16"
  },
  "preferGlobal": true,
  "scripts": {
    "build": "unbuild",
    "dev": "tsx ./src/cli.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp && pnpm publish --no-git-checks",
    "test": "node --test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@pnpm/lockfile-file": "^3.2.1",
    "@pnpm/logger": "^3.2.3",
    "@yarnpkg/lockfile": "^1.1.0",
    "chalk": "^4.1.2",
    "commander": "^7.2.0",
    "fs-extra": "^9.1.0",
    "node-fetch": "^2.7.0",
    "node-releases": "^2.0.18",
    "semver": "^7.6.3",
    "yocto-spinner": "^0.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@antfu/utils": "^0.5.2",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.19.54",
    "@types/node-fetch": "^2.6.11",
    "@types/semver": "^7.5.8",
    "@types/yarnpkg__lockfile": "^1.1.9",
    "bumpp": "^8.2.1",
    "eslint": "^9.13.0",
    "lint-staged": "^15.2.10",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.19.1",
    "typescript": "^4.9.5",
    "unbuild": "^0.8.11"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,md}": [
      "eslint --fix"
    ]
  }
}
